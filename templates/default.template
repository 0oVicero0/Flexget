<html>
  <head>
    <style type="text/css">
    </style>    

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>FlexGet</title>
  </head>

  <body>

  #for $entry in $accepted
  <div>

    <!-- just random testing -->

    <div>
      <h2><a href="$entry.url">$entry.title</a></h2>
    </div>

    <!-- the actial logic could be something like -->

    #if 'series_name' in $entry
      <div class="series">
      $entry.series_name
      </div>
    #else if 'imdb_url' in $entry
      <div class="movie">
      $entry.title
      </div>
    #end if

  </div>
  #end for

  </body>
</html>
